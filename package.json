{
  "name": "ticketscapler",
  "version": "1.0.0",
  "description": "Event planning and hosting application 2021\r  \"Great seats don't come cheap\"",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KENSTONEJAY/TicketScapler.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KENSTONEJAY/TicketScapler/issues"
  },
  "homepage": "https://github.com/KENSTONEJAY/TicketScapler#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "bootstrap": "^5.0.2",
    "dotenv": "^10.0.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mysql2": "^2.2.5",
    "react-router-dom": "^5.2.0",
    "sequelize": "^6.3.5"
  }
}
